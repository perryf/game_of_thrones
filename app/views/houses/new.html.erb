<h2>Create a new House</h2>
<%= form_for @house do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :photo_url %>
  <%= f.text_field :photo_url %>

  <%= f.label :motto %>
  <%= f.text_field :motto %>

  <%= f.label :territory %>
  <%= f.text_field :territory %>

  <%= f.submit %>
<% end %>
