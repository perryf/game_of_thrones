<h2>Edit This House</h2>
<%= form_for @house do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :photo_url %>
  <%= f.text_field :photo_url %>

  <%= f.label :motto %>
  <%= f.text_field :motto %>

  <%= f.label :territory %>
  <%= f.text_field :territory %>

  <%= f.submit %>
<% end %>
<br>
<%= link_to 'Delete this House', house_path(@house), method: :delete, class: 'delete' %>
